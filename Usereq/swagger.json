"{\n  \"openapi\": \"3.0.0\",\n  \"info\": {\n    \"version\": \"1.2.0\",\n    \"title\": \"BeThere - OpenAPI 3.0\",\n    \"description\": \"BeThere app. Connect with events near you.\",\n    \"contact\": {\n      \"name\": \"Kougias Spyros\\nPapadopoulos Giannis\\nHadjilyras Giorgos\\nFotiadis Konstantinos\\n\",\n      \"email\": \"kspyrido@ece.auth.gr\"\n    },\n    \"license\": {\n      \"name\": \"Apache 2.0\",\n      \"url\": \"https://www.apache.org/licenses/LICENSE-2.0.html\"\n    }\n  },\n  \"servers\": [\n    {\n      \"url\": \"http://r4a-platform.ddns.net:8678\"\n    }\n  ],\n  \"tags\": [\n    {\n      \"name\": \"user\",\n      \"description\": \"Operations about user\"\n    },\n    {\n      \"name\": \"map/calendar\",\n      \"description\": \"Open/Import the map/calendar\"\n    },\n    {\n      \"name\": \"event\",\n      \"description\": \"Operations about events\"\n    },\n    {\n      \"name\": \"search\",\n      \"description\": \"Search section, filters and operations\"\n    },\n    {\n      \"name\": \"notification/email\",\n      \"description\": \"send notification/email\"\n    },\n    {\n      \"name\": \"report\",\n      \"description\": \"Send report (user or event)\"\n    }\n  ],\n  \"paths\": {\n    \"/user/{username}\": {\n      \"get\": {\n        \"tags\": [\n          \"user\"\n        ],\n        \"description\": \"FR35 - User must be able to select a User\\n\",\n        \"operationId\": \"selectUser\",\n        \"parameters\": [\n          {\n            \"name\": \"username\",\n            \"description\": \"user selected\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Return User\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/User\"\n                }\n              }\n            }\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/user/{username}/follow\": {\n      \"put\": {\n        \"tags\": [\n          \"user\"\n        ],\n        \"description\": \"FR1 - User must be able to press the \\\"Follow\\\" button\\n\",\n        \"operationId\": \"followUser\",\n        \"parameters\": [\n          {\n            \"name\": \"username\",\n            \"description\": \"User that you will follow/unfollow\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Ok User is now followed/unfollowed\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/user/{username}/ban\": {\n      \"put\": {\n        \"tags\": [\n          \"user\"\n        ],\n        \"description\": \"FR23 - The admin must be able to press the \\\"Ban\\\" button\\n\",\n        \"operationId\": \"banUser\",\n        \"parameters\": [\n          {\n            \"name\": \"username\",\n            \"description\": \"User that will be banned\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          },\n          {\n            \"name\": \"confirmed\",\n            \"description\": \"Confirmation parameter 0 -> 400 not banned\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"boolean\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"User banned\"\n          },\n          \"400\": {\n            \"description\": \"confirmed = 0 -> not banned\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/user/{username}/restrict\": {\n      \"put\": {\n        \"tags\": [\n          \"user\"\n        ],\n        \"description\": \"FR22 - The admin must be able to press the \\\"Restrict\\\" button\\n\",\n        \"operationId\": \"restrictUser\",\n        \"parameters\": [\n          {\n            \"name\": \"username\",\n            \"description\": \"User that will be restricted/unrestricted\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          },\n          {\n            \"name\": \"confirmed\",\n            \"description\": \"confirmation about user restriction 0 -> not allowed\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"boolean\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Ok User is now restricted/unrestricted\"\n          },\n          \"400\": {\n            \"description\": \"confirmed = 0 -> not restricted/unrestricted\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/user/{username}/location\": {\n      \"put\": {\n        \"tags\": [\n          \"user\"\n        ],\n        \"description\": \"FR19 - The system must be able to ask for location permission.\\n\",\n        \"operationId\": \"askUserForLocationPermission\",\n        \"parameters\": [\n          {\n            \"name\": \"username\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          },\n          {\n            \"name\": \"confirmed\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"boolean\",\n              \"description\": \"confirmation about location access 0 -> not allowed\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"location access allowed\"\n          },\n          \"400\": {\n            \"description\": \"confirmed = 0 -> not allowed\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/user/{username}/fileaccess\": {\n      \"put\": {\n        \"tags\": [\n          \"user\"\n        ],\n        \"description\": \"FR37 - The system must be able to ask for file access permission.\\n\",\n        \"operationId\": \"askUserForFilePermission\",\n        \"parameters\": [\n          {\n            \"name\": \"username\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          },\n          {\n            \"name\": \"confirmed\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"boolean\",\n              \"description\": \"confirmation about file access 0 -> not allowed\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"File access allowed\"\n          },\n          \"400\": {\n            \"description\": \"confirmed = 0 -> not allowed\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/event\": {\n      \"post\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR7 - User must be able create an event\\nFR20 - System must be able to assign a user as a host\\n\",\n        \"operationId\": \"createEvent\",\n        \"parameters\": [\n          {\n            \"name\": \"username\",\n            \"description\": \"user that created the event\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          }\n        ],\n        \"requestBody\": {\n          \"description\": \"Create new Event\",\n          \"content\": {\n            \"application/json\": {\n              \"schema\": {\n                \"$ref\": \"#/components/schemas/NewEvent\"\n              }\n            }\n          }\n        },\n        \"responses\": {\n          \"201\": {\n            \"description\": \"Return the event\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Event\"\n                }\n              }\n            }\n          },\n          \"400\": {\n            \"description\": \"A required field was not completed\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/event/{eventid}\": {\n      \"delete\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR12 - Host must be able to delete their event\\nFR24 - Admin must be able to delete an event\\n\",\n        \"operationId\": \"deleteEvent\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"description\": \"Event that will be deleted\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          },\n          {\n            \"name\": \"username\",\n            \"description\": \"User that will delete the event\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          },\n          {\n            \"name\": \"confirmed\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"boolean\",\n              \"description\": \"Confirmation parameter 0 -> 400 not delete\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"204\": {\n            \"description\": \"Event deleted\"\n          },\n          \"400\": {\n            \"description\": \"confirmed = 0 -> not deleted\"\n          },\n          \"default\": {\n            \"description\": \"Error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      },\n      \"put\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR8 - Host must be able to edit their event\\n\",\n        \"operationId\": \"editEvent\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"description\": \"event that will be edited\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          }\n        ],\n        \"requestBody\": {\n          \"$ref\": \"#/components/requestBodies/NewEvent\"\n        },\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Event edited and returned\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Event\"\n                }\n              }\n            }\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      },\n      \"get\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR33 - User must be able to select an event\\n\",\n        \"operationId\": \"selectEvent\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"description\": \"event that will be edited\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          },\n          {\n            \"name\": \"username\",\n            \"description\": \"User that will delete the event\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Event edited and returned\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Event\"\n                }\n              }\n            }\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/event/{eventid}/comment\": {\n      \"post\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR13 - User must be able to comment an event\\n\",\n        \"operationId\": \"postComment\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          },\n          {\n            \"name\": \"username\",\n            \"description\": \"user that created the event\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          }\n        ],\n        \"requestBody\": {\n          \"description\": \"Create new Comment\",\n          \"content\": {\n            \"application/json\": {\n              \"schema\": {\n                \"$ref\": \"#/components/schemas/NewComment\"\n              }\n            }\n          }\n        },\n        \"responses\": {\n          \"201\": {\n            \"description\": \"Comment has been posted\"\n          },\n          \"400\": {\n            \"description\": \"A required field was not completed\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/event/{eventid}/comment/{commentid}\": {\n      \"delete\": {\n        \"description\": \"R14 - User must be able to delete their comment\\nR25 - Admin must be able to delete a comment\\nR34 - Host must be able to delete a comment from their event\\n\",\n        \"tags\": [\n          \"event\"\n        ],\n        \"operationId\": \"deleteComment\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          },\n          {\n            \"name\": \"username\",\n            \"description\": \"User that will delete the comment\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          },\n          {\n            \"name\": \"commentid\",\n            \"in\": \"path\",\n            \"description\": \"Comment that will be deleted\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          },\n          {\n            \"name\": \"confirmed\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"boolean\",\n              \"description\": \"Confirmation parameter 0 -> 400 not delete\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"204\": {\n            \"description\": \"Event deleted\"\n          },\n          \"400\": {\n            \"description\": \"confirmed = 0 -> not deleted\"\n          },\n          \"default\": {\n            \"description\": \"Error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      },\n      \"put\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR15 - Host must be able to pin a comment on their event\\n\",\n        \"operationId\": \"pinComment\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          },\n          {\n            \"name\": \"username\",\n            \"description\": \"User that will pin the event\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          },\n          {\n            \"name\": \"commentid\",\n            \"in\": \"path\",\n            \"description\": \"Comment that will be pinned\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Comment pinned and return event\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Event\"\n                }\n              }\n            }\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/event/{eventid}/announcement\": {\n      \"post\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR16 - Host must be able to make an announcement about their event\\n\",\n        \"operationId\": \"postAnnouncment\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          },\n          {\n            \"name\": \"username\",\n            \"description\": \"users that the announcement will be sent to\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"array\",\n              \"items\": {\n                \"type\": \"string\"\n              }\n            }\n          }\n        ],\n        \"requestBody\": {\n          \"description\": \"Create new Announcement\",\n          \"content\": {\n            \"application/json\": {\n              \"schema\": {\n                \"$ref\": \"#/components/schemas/Announcement\"\n              }\n            }\n          }\n        },\n        \"responses\": {\n          \"201\": {\n            \"description\": \"Announcement has been posted\"\n          },\n          \"400\": {\n            \"description\": \"A required field was not completed\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/event/{eventid}/photo\": {\n      \"post\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR38 - The host must be able to upload photos to their event.\\n\",\n        \"operationId\": \"uploadPhoto\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          },\n          {\n            \"name\": \"username\",\n            \"description\": \"Host\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          },\n          {\n            \"name\": \"photoData\",\n            \"description\": \"The photo\",\n            \"in\": \"query\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"string\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"201\": {\n            \"description\": \"Photos uploaded\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      },\n      \"get\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR39 - The user must be able to cycle through event photos.\\n\",\n        \"operationId\": \"getNextOrPreviousPhoto\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Get photo\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Event\"\n                }\n              }\n            }\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/event/{eventid}/interested\": {\n      \"put\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR5 - The user must be able to press the \\\"Interested In\\\" button.\\n\",\n        \"operationId\": \"interestedInEvent\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Interested in event\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/event/{eventid}/vouch\": {\n      \"put\": {\n        \"tags\": [\n          \"event\"\n        ],\n        \"description\": \"FR10 - The user must be able to press the \\\"Vouch\\\" button.\\n\",\n        \"operationId\": \"vouchForEvent\",\n        \"parameters\": [\n          {\n            \"name\": \"eventid\",\n            \"in\": \"path\",\n            \"required\": true,\n            \"schema\": {\n              \"type\": \"integer\"\n            }\n          }\n        ],\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Vouched for event\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/report\": {\n      \"post\": {\n        \"tags\": [\n          \"report\"\n        ],\n        \"description\": \"FR6 - The user must be able to report a user.\\nFR9 - The user must be able to report an event.\\n\",\n        \"operationId\": \"postReport\",\n        \"requestBody\": {\n          \"description\": \"Create new Report\",\n          \"content\": {\n            \"application/json\": {\n              \"schema\": {\n                \"$ref\": \"#/components/schemas/Report\"\n              }\n            }\n          }\n        },\n        \"responses\": {\n          \"201\": {\n            \"description\": \"Report successfull\"\n          },\n          \"409\": {\n            \"description\": \"Already reported\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\n      }\n    },\n    \"/map\": {\n      \"put\": {\n        \"tags\": [\n          \"map/calendar\"\n        ],\n        \"description\": \"FR2 - The user must be able to interact with map.\\n\",\n        \"operationId\": \"interactWithMap\",\n        \"responses\": {\n          \"200\": {\n            \"description\": \"Interact with map (Google)\"\n          },\n          \"default\": {\n            \"description\": \"Unexpected error\",\n            \"content\": {\n              \"application/json\": {\n                \"schema\": {\n                  \"$ref\": \"#/components/schemas/Error\"\n                }\n              }\n            }\n          }\n        }\